<p id="notice"><%= notice %></p>

<h1>Infomations</h1>

<table>
  <thead>
    <tr>
      <th>Info</th>
      <th>About</th>
      <th>Start</th>
      <th>End</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @infomations.each do |infomation| %>
      <tr>
        <td><%= infomation.info %></td>
        <td><%= infomation.about %></td>
        <td><%= infomation.start %></td>
        <td><%= infomation.end %></td>
        <td><%= link_to 'Show', infomation %></td>
        <td><%= link_to 'Edit', edit_infomation_path(infomation) %></td>
        <td><%= link_to 'Destroy', infomation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Infomation', new_infomation_path %>
